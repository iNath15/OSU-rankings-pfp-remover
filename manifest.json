{
    "manifest_version": 3,
    "name": "OSU Rankings pfp remover",
    "version": "1.0",
    "description": "Removes profile pictures from the rankings page.",
    "permissions": ["scripting", "activeTab"],
    "content_scripts": [
      {
        "matches": ["*://osu.ppy.sh/*"],
        "js": ["remove.js"]
      }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
  }
  
